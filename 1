        int i,m=0,flag=0;      
  int n=3;//HERE YOU PUT IN A NUMBER WHICH YOU WANT TO BE CHECKED   
  m=n/2;      
  if(n==0||n==1){  
   System.out.println(n+" NOT A PRIME NUMBER");      
  }else{  

    //START OF ELSE
   for(i=2;i<=m;i++){      
    if(n%i==0){      
     System.out.println(n+" NOT PRIME NUMBER");      
     flag=1;      
     break;      
    }      
   }      
   if(flag==0)  { System.out.println(n+" IS A PRIME NUMBER"); }  
  }
  //   /\END OF ELSE





      
