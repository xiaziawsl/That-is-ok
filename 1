        System.out.println("++VAR Analysis");
        
        for (int i=0; i<5; i++)
        {
            System.out.println(i);
        }
        
        System.out.println("VAR++ Analysis");
        
        
        //Explanation: Although both var++ and ++var increment the variable they are applied to, 
        //the result returned by var++ is the value of the variable before incrementing, 
        //whereas the result returned by ++var is the value of the variable after the increment
        //is applied
        
        
        
        
        //However if you write 
        int x = 6;
        assert ++x == 7;
        System.out.println(x);
        //that'll work
        
        //But if you type
        int y = 6;
        assert y++ == 7;
        System.out.println(y);
        
        
        //It fails.
        
        
        boolean pass_this_class=false;
